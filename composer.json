{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "aws/aws-php-sns-message-validator": "^1.5",
        "aws/aws-sdk-php": "^3.63",
        "doctrine/annotations": "^1.6",
        "doctrine/couchdb": "^1.0@beta",
        "doctrine/dbal": "^2.8",
        "fideloper/proxy": "^4.0",
        "fluent/logger": "^1.0",
        "fzaninotto/faker": "^1.8",
        "graylog2/gelf-php": "^1.5",
        "guzzlehttp/guzzle": "^6.3",
        "hoa/bench": "3.17.01.11",
        "hoa/console": "^3.17",
        "hoa/router": "3.17.01.14",
        "ircmaxell/random-lib": "^1.2",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "league/flysystem-cached-adapter": "^1.0",
        "league/flysystem-eventable-filesystem": "^1.0",
        "league/flysystem-rackspace": "^1.0",
        "league/flysystem-sftp": "^1.0",
        "league/flysystem-webdav": "^1.0",
        "league/flysystem-ziparchive": "^1.0",
        "mdanter/ecc": "^0.5.0",
        "moontoast/math": "^1.1",
        "nexmo/client": "^1.4",
        "packfire/php5.3-compat": "^1.0",
        "pda/pheanstalk": "^3.1",
        "php-amqplib/php-amqplib": "^2.7",
        "php-console/php-console": "^3.1",
        "php-http/httplug": "^1.1",
        "phpunit/php-invoker": "^2.0",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^3.1",
        "ramsey/uuid-console": "^1.1",
        "ramsey/uuid-doctrine": "^1.4",
        "rollbar/rollbar": "^1.5",
        "ruflin/elastica": "^6.0",
        "sentry/sentry": "^1.9",
        "spatie/flysystem-dropbox": "^1.0",
        "srmklive/flysystem-dropbox-v2": "^1.0",
        "symfony/browser-kit": "^4.1",
        "symfony/config": "^4.1",
        "symfony/dependency-injection": "^4.1",
        "symfony/dom-crawler": "^4.1",
        "symfony/expression-language": "^4.1",
        "symfony/lock": "^4.1",
        "symfony/proxy-manager-bridge": "^4.1",
        "symfony/psr-http-message-bridge": "^1.0",
        "symfony/yaml": "^4.1",
        "true/punycode": "^2.1",
        "whoops/soap": "^1.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
